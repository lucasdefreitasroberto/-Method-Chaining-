unit Unit1;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants,
  System.Classes, Vcl.Graphics, Vcl.Controls, Vcl.Forms, Vcl.Dialogs,
  Vcl.StdCtrls, Vcl.Buttons, interfaces, classes, implementa;  // Adicione aqui a unidade 'classes' e 'implementa'

type
  TForm1 = class(TForm)
    AAA: TBitBtn;
    procedure AAAClick(Sender: TObject);
  private
    { Private declarations }
  public
    procedure TestFluentInterface;
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

{ TForm1 }

procedure TForm1.AAAClick(Sender: TObject);
begin
  Self.TestFluentInterface();
end;

procedure TForm1.TestFluentInterface;
var
  Doc: IDocumento;
begin
  Doc := TDocumento.New.Documento.Nome('Lucas').Idade(30).Endereco('Rua Sebastião Telles da Costa').&End;

  ShowMessage(Doc.InfoDocumento);
end;

end.

